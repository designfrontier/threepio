{
  "name": "threepio",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "version": "1.0.0",
  "description": "I make code review easier for humans",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --cache --ext .tsx,.ts,.js,.jsx .",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\" && npm run build",
    "prettier": "prettier -w .",
    "test": "jest"
  },
  "files": [
    "lib/**/*",
    "bin/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/designfrontier/threepio.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/designfrontier/threepio/issues"
  },
  "homepage": "https://github.com/designfrontier/threepio#readme",
  "preferGlobal": true,
  "devDependencies": {
    "@types/jest": "29.0.3",
    "@types/node": "18.7.18",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "@typescript-eslint/typescript-estree": "^5.38.0",
    "case-anything": "^2.1.13",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-prettier": "0.0.0-empty",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "prompts": "^2.4.2",
    "ts-jest": "29.0.1",
    "typescript": "4.8.3"
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint"
  },
  "dependencies": {
    "change-case": "^5.1.2",
    "yaml": "^2.3.3"
  }
}
